@model IEnumerable<CovidApi.Models.DataFile>

@{
    ViewData["Area"] = "Admin";
    ViewData["SubArea"] = "DataFiles";
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="table-responsive">
    <table id="dataFilesDataTable" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FileName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.RecordsProcessed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Completed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CompletedAt)
                </th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
 </div>
@section Scripts
{
    <script src="~/js/dataTables/dataFilesDataTable.js"></script>
}
